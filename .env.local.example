# Keycloak Configuration
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8080
NEXT_PUBLIC_KEYCLOAK_REALM=Customer
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=customer-web

# Developer Realm Configuration
NEXT_PUBLIC_KEYCLOAK_DEV_REALM=developer
NEXT_PUBLIC_KEYCLOAK_DEV_CLIENT_ID=dev-web

# Vendor Realm Configuration  
NEXT_PUBLIC_KEYCLOAK_VENDOR_REALM=vendor
NEXT_PUBLIC_KEYCLOAK_VENDOR_CLIENT_ID=vendor-web

# Keycloak Admin Credentials
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=your-admin-password

# Google OAuth Configuration
NEXT_PUBLIC_GOOGLE_IDP_ALIAS=google

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Security Configuration
KEYCLOAK_REQUIRE_TOTP=false

# CSRF Protection (generate a secure random string)
CSRF_SECRET=your-csrf-secret-key

# Session Secret (generate a secure random string)
SESSION_SECRET=your-session-secret-key