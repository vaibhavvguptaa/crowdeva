# Production Environment Variables for www.crowdeval.com

# Keycloak Configuration
KEYCLOAK_DOMAIN=www.crowdeval.com
NEXT_PUBLIC_KEYCLOAK_URL=https://www.crowdeval.com/auth
NEXT_PUBLIC_KEYCLOAK_REALM=Customer
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=customer-web
NEXT_PUBLIC_KEYCLOAK_DEV_REALM=developer
NEXT_PUBLIC_KEYCLOAK_VENDOR_REALM=vendor
NEXT_PUBLIC_KEYCLOAK_DEV_CLIENT_ID=dev-web
NEXT_PUBLIC_KEYCLOAK_VENDOR_CLIENT_ID=vendor-web

# Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password
MYSQL_PASSWORD=your_secure_mysql_password

# Keycloak Admin
KEYCLOAK_ADMIN_PASSWORD=your_secure_admin_password

# Security Configuration
CSRF_SECRET=your_secure_csrf_secret
SESSION_SECRET=your_secure_session_secret

# CORS Configuration
ALLOWED_ORIGIN=https://www.crowdeval.com

# Email Configuration
SUPPORT_EMAIL=support@crowdeval.com